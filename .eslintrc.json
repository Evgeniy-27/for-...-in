{
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly" 
},
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "parserOptions": { 
    "ecmaVersion": 2018, 
    "sourceType": "module"
}, 
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}